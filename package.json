{
    "name": "simple-boilerplate",
    "version": "1.0.0",
    "private": true,
    "description": "Simple Boilerplate",
    "keywords": [
        "gatsby"
    ],
    "scripts": {
        "dev:gatsby": "gatsby develop -H 0.0.0.0",
        "build": "NODE_ENV=production yarn build:css && gatsby build",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "build:css": "tailwindcss -i ./src/styles/tailwind/base.css -o ./src/styles/tailwind/build.css",
        "watch:css": "tailwindcss -i ./src/styles/tailwind/base.css -o ./src/styles/tailwind/build.css --watch",
        "dev": "NODE_ENV=development concurrently \"yarn dev:gatsby\" \"yarn watch:css\""
    },
    "dependencies": {
        "autoprefixer": "^10.4.8",
        "browser-monads": "^1.0.0",
        "classnames": "^2.3.1",
        "gatsby": "^4.24.4",
        "gatsby-plugin-alias-imports": "^1.0.5",
        "gatsby-plugin-image": "^2.21.0",
        "gatsby-plugin-manifest": "^4.24.0",
        "gatsby-plugin-preact": "^6.21.0",
        "gatsby-plugin-react-helmet": "^5.21.0",
        "gatsby-plugin-react-helmet-canonical-urls": "^1.4.0",
        "gatsby-plugin-remove-trailing-slashes": "^4.19.0",
        "gatsby-plugin-sharp": "^4.21.0",
        "gatsby-plugin-sitemap": "^5.21.0",
        "gatsby-source-filesystem": "^4.21.0",
        "gatsby-transformer-json": "^4.21.0",
        "gatsby-transformer-sharp": "^4.21.0",
        "postcss": "^8.4.16",
        "preact": "^10.10.6",
        "preact-render-to-string": "^5.2.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-helmet": "^6.1.0",
        "react-icons": "^4.4.0",
        "react-intersection-observer": "^9.4.0",
        "react-multi-carousel": "^2.8.2",
        "tailwindcss": "^3.1.8"
    },
    "devDependencies": {
        "concurrently": "^7.3.0",
        "dotenv": "^16.0.1"
    }
}
